<apex:page standardController="Employee__c" recordSetVar="Employees">
    
    <apex:pageBlock title="Employee List">
        <apex:pageBlockTable value="{!Employees}" var="Emp" cellpadding="4">
        <apex:column value="{!Emp.Full_Name__c}"/>
        <apex:column value="{!Emp.Email__c}"/>    
        <apex:column value="{!Emp.Contact__c}"/>     
         <apex:column value="{!Emp.Country__c}"/>     
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>